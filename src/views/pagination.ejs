<% if (pages > 1) { %>
    <nav class="nav justify-content-center">
        <ul class="pagination">
            <% if (current == 1) { %>
                <li class="page-item disabled">
                    <a class="page-link" href="<%= url + "?page=1" %>">First</a>
                </li>
            <% } else { %>
                <li class="page-item">
                    <a class="page-link" href="<%= url + "?page=1" %>">First</a>
                </li>
            <% } %>
            <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
            <% if (i !== 1) { %>
                <li class="page-item">
                    <span class="page-link">... </span>
                </li>
            <% } %>

            <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                <% if (i == current) { %>
                    <li class="page-item active">
                        <a class="page-link" href="<%= url + "?page=" + i %>"><%= i %> <span
                                    class="sr-only">(current)</span></a>
                    </li>
                <% } else { %>
                    <li class="page-item"><a class="page-link" href="<%= url + "?page=" + i %>"> <%= i %></a></li>
                <% } %>
            <% } %>
            <% if ( current + 4 < pages) { %>
                <li class="page-item">
                    <span class="page-link">... </span>
                </li>

            <% } %>
            <% if (current == pages) { %>
                <li class="page-item disabled">
                    <a class="page-link" href="<%= url + "?page=" + pages %>">Last</a>
                </li>
            <% } else { %>
                <li class="page-item">
                    <a class="page-link" href="<%= url + "?page=" + pages %>">Last</a>
                </li>
            <% } %>
        </ul>
    </nav>

<% } %>
